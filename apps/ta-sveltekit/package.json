{
	"name": "ta-sveltekit",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"prettier": "@niama/config-prettier",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"preview": "vite preview",
		"sanity:build": "sanity build",
		"sanity:deploy": "sanity deploy",
		"sanity:deploy-graphql": "sanity graphql deploy",
		"sanity:dev": "sanity start"
	},
	"dependencies": {
		"@brainandbones/skeleton": "^0.76.0",
		"@fontsource/raleway": "^4.5.11",
		"@iconify/json": "^2.1.142",
		"@mailchimp/mailchimp_marketing": "^3.0.80",
		"@niama/core": "workspace:*",
		"@portabletext/svelte": "^1.0.1",
		"@sanity/client": "^3.4.1",
		"@splidejs/svelte-splide": "^0.2.9",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-is": "^18.2.0",
		"sanity": "3.0.0-rc.2",
		"sanity-typed-schema-builder": "^1.15.11",
		"styled-components": "^5.3.6",
		"zod": "^3.19.1"
	},
	"devDependencies": {
		"@niama/config-prettier": "workspace:*",
