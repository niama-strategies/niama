		"@sveltejs/kit": "next",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.8",
		"eslint-config-niama": "workspace:*",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"unplugin-icons": "^0.14.14",
		"vite": "^3.1.0"
	}
